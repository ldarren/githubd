{
    "deps":{
        "act":"./action",
        "github":"./github",
        "slack":"./slack",
		"web": ["context", "webServer"]
    },
    "routes":{
		"ERR/#msg":[
            ["act","print","res",["unexpected error"]],
            []
		],
		"END/#msg":[
            ["act","print","res",["unexpected end"]],
            []
		],
        "/auth":[
            ["github","getAuth","query","res"],
            []
		],
        "/setup":[
            ["act","print","res",["This is URL to redirect users to after they install your app"]],
            []
		],
        "/gh/hook":[
			["web", "getBody", "req", "body"],
            ["github","webhooks","req", "body"],
            []
		],
        "/slack":[
            ["slack","oauth", "query"],
            []
		],
        "/slack/hook":[
			["web", "getBody", "req", "body"],
            ["slack","verify","body", ":syntax"],
            ["slack","webhooks","body", ":syntax"],
            []
		],
        "/repos/:method/:accept/#path":[
            ["github","repos","query","res"],
            []
		],
        "/raw/#path":[
            ["github","raw","res"],
            []
		]
    }
}
